<h1>New Product</h1>
<ul>
  <% @products.each do |product| %>
    <li><%= product.title %></li>
    <p><%= product.description%></p>
    <p><%= product.variant.sku%></p>
    <p><%= product.variant.stock_quantity%></p>

    <%= link_to 'Edit', edit_product_path(product) %> |

  <% end %>
</ul>
